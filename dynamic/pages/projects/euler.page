    <pre id="problem_13" class = "solution">
    def problem_13():
        f = open('Data13.csv','r').readlines()
        Sum,Answer=0,[]
        for i in range(0,50):
            for line in f:
                Sum+= int(line.strip()[49-i])
            Answer.append(str(Sum%10))
            Sum=(Sum-Sum%10)/10
